	minSpeed(float) : "Minimum speed (Snowfall only)" : 25 : "Minimum speed Snowfall particles descend at."
	maxSpeed(float) : "Maximum speed (Snowfall only)" : 35 : "Maximum speed Snowfall particles descend at."
	input Alpha(integer) : "If Precipitation Type is Rain, sets the density."
]
